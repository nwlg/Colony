                        </td>
                    </tr>
                    <tr class="nif">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.nif"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.nif}"/>
                           <input type="text" name="nif" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.nif.title')" onmouseout="hints.hide()"  id="continuarNif"/>
                        </td>
                    </tr>
                    <tr class="email">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.email"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.email}"/>
