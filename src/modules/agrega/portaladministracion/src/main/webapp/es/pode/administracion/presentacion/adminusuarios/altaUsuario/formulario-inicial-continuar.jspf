                        </td>
                    </tr>
                    <tr class="openIdUrl">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.open.id.url"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.openIdUrl}"/>
                           <input type="text" name="openIdUrl" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.open.id.url.title')" onmouseout="hints.hide()"  id="continuarOpenIdUrl"/>
                        </td>
                    </tr>
                    <tr class="idioma">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.idioma"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.idioma}"/>
                           <c:choose>
                               <c:when test="${!empty form.idiomaBackingList}">
                                  <html:select name="form" property="idioma" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.idioma.title')" onmouseout="hints.hide()"  styleId="continuarIdioma">
                                      <html:optionsCollection name="form" property="idiomaBackingList" label="label" value="value"/>
                                  </html:select>
                               </c:when>
                               <c:otherwise>
                                   <html:select name="form" property="idioma" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.idioma.title')" onmouseout="hints.hide()"  styleId="continuarIdioma"/>
                               </c:otherwise>
                           </c:choose>
                        </td>
                    </tr>
                    <tr class="idiomaBusqueda">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.idioma.busqueda"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.idiomaBusqueda}"/>
                           <c:choose>
                               <c:when test="${!empty form.idiomaBusquedaBackingList}">
                                  <html:select name="form" property="idiomaBusqueda" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.idioma.busqueda.title')" onmouseout="hints.hide()"  styleId="continuarIdiomaBusqueda">
                                      <html:optionsCollection name="form" property="idiomaBusquedaBackingList" label="label" value="value"/>
                                  </html:select>
                               </c:when>
                               <c:otherwise>
                                   <html:select name="form" property="idiomaBusqueda" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.idioma.busqueda.title')" onmouseout="hints.hide()"  styleId="continuarIdiomaBusqueda"/>
