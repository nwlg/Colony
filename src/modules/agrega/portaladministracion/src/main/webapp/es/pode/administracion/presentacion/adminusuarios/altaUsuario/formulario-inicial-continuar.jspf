<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.tipoCatalogador}"/>
                           <input type="text" name="tipoCatalogador" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.tipo.catalogador.title')" onmouseout="hints.hide()"  id="continuarTipoCatalogador"/>
                        </td>
                    </tr>
                    <tr class="cuota">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.cuota"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.cuota}"/>
                           <input type="text" name="cuota" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.cuota.title')" onmouseout="hints.hide()"  id="continuarCuota"/>
                        </td>
                    </tr>

                    <tr>
                        <td>
                              <html:submit onmouseover="hints.show('Continuar')" onmouseout="hints.hide()" styleId="form_submit">
                                  <bean:message key="alta.usuario.formulario.inicial.continuar"/>
                              </html:submit>	
                        </td>
                    </tr>
                </tbody>
            </table>
        </html:form>
    </div>

</div>
 <br class="clr"/>
