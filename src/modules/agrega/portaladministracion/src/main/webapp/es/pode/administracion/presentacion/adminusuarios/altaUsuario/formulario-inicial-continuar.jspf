                        </td>
                    </tr>
                    <tr class="usuario">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.usuario"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.usuario}"/>
                           <input type="text" name="usuario" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.usuario.title')" onmouseout="hints.hide()"  id="continuarUsuario"/>
                        </td>
                    </tr>
                    <tr class="clave">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.clave"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.clave}"/>
                           <input type="password" name="clave" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.clave.title')" onmouseout="hints.hide()"  id="continuarClave"/>
                        </td>
                    </tr>
                    <tr class="repitaClave">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.repita.clave"/></td>
