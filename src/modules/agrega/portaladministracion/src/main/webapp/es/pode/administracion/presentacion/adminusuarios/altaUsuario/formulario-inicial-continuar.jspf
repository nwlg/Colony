<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.tipoEmpaquetador}"/>
                           <c:choose>
                               <c:when test="${!empty form.tipoEmpaquetadorBackingList}">
                                  <html:select name="form" property="tipoEmpaquetador" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.tipo.empaquetador.title')" onmouseout="hints.hide()"  styleId="continuarTipoEmpaquetador">
                                      <html:optionsCollection name="form" property="tipoEmpaquetadorBackingList" label="label" value="value"/>
                                  </html:select>
                               </c:when>
                               <c:otherwise>
                                   <html:select name="form" property="tipoEmpaquetador" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.tipo.empaquetador.title')" onmouseout="hints.hide()"  styleId="continuarTipoEmpaquetador"/>
                               </c:otherwise>
                           </c:choose>
                        </td>
                    </tr>
                    <tr class="tipoCatalogador">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.tipo.catalogador"/></td>
