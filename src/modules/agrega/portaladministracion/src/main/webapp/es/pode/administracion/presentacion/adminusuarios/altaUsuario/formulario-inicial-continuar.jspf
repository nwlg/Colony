<%@ include file="/taglib-imports.jspf" %>
<div id="continuar" class="action">
    <h3><bean:message key="alta.usuario.formulario.inicial.continuar"/></h3>
    <div class="trigger">
        <html:form styleId="altaUsuarioFormularioInicialContinuarForm" action="/AltaUsuario/FormularioInicialContinuar" method="post" enctype="multipart/form-data" onsubmit="">
            <table>
                <tbody>
                    <tr class="nombre">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.nombre"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.nombre}"/>
                           <input type="text" name="nombre" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.nombre.title')" onmouseout="hints.hide()"  id="continuarNombre"/>
                        </td>
                    </tr>
                    <tr class="apellido1">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.apellido1"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.apellido1}"/>
                           <input type="text" name="apellido1" value="${value}" onmouseover="hints.show('alta.usuario.formulario.inicial.continuar.param.apellido1.title')" onmouseout="hints.hide()"  id="continuarApellido1"/>
                        </td>
                    </tr>
                    <tr class="apellido2">
                        <td class="label"><bean:message key="alta.usuario.formulario.inicial.continuar.param.apellido2"/></td>
