<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans
    PUBLIC "-//SPRING//DTD BEAN//EN"
           "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="beanRefFactory"
         class="org.springframework.context.support.ClassPathXmlApplicationContext">
        <constructor-arg>
           <list>
                <value>user-applicationContext.xml</value>
                <value>applicationContext-acegiSecurity.xml</value>
		<value>applicationContext-acegiSecurity-frontend.xml</value>
                <value>applicationContext.xml</value>
                <value>applicationContext-dataSource.xml</value>                
                    <!-- beanRefFactory merge-point -->

           </list>
        </constructor-arg>
    </bean>

</beans>
