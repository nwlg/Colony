<%@ include file="/taglib-imports.jspf" %>
<div id="modificarGeneral" class="action">
    <h3><bean:message key="catalogador.avanzado.categorias.form.modificar.general"/></h3>
    <div class="trigger">
        <html:form styleId="catalogadorAvanzadoCategoriasFormModificarGeneralForm" action="/CatalogadorAvanzado/CategoriasFormModificarGeneral" method="post" enctype="multipart/form-data" onsubmit="">
            <table>
                <tbody>
                    <tr class="identificador">
                        <td class="label"><bean:message key="catalogador.avanzado.categorias.form.modificar.general.param.identificador"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.identificador}"/>
                           <input type="text" name="identificador" value="${value}" onmouseover="hints.show('catalogador.avanzado.categorias.form.modificar.general.param.identificador.title')" onmouseout="hints.hide()"  id="modificarGeneralIdentificador"/>
                        </td>
                    </tr>
                    <tr class="returnURL">
                        <td class="label"><bean:message key="catalogador.avanzado.categorias.form.modificar.general.param.return.url"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.returnURL}"/>
                           <input type="text" name="returnURL" value="${value}" onmouseover="hints.show('catalogador.avanzado.categorias.form.modificar.general.param.return.url.title')" onmouseout="hints.hide()"  id="modificarGeneralReturnURL"/>
                        </td>
                    </tr>
                    <tr class="datos">
                        <td class="label"><bean:message key="catalogador.avanzado.categorias.form.modificar.general.param.datos"/></td>
                        <td class="field">
<!--RLA ${field.ajaxParamType} -->
		
                           <c:set var="value" value="${form.datos}"/>
                           <c:choose>
                               <c:when test="${!empty form.datosBackingList}">
                                  <html:select name="form" property="datosAsArray" onmouseover="hints.show('catalogador.avanzado.categorias.form.modificar.general.param.datos.title')" onmouseout="hints.hide()"  multiple="multiple" styleId="modificarGeneralDatos">
                                      <html:optionsCollection name="form" property="datosBackingList" label="label" value="value"/>
                                  </html:select>
                               </c:when>
                               <c:otherwise>
                                   <html:select name="form" property="datosAsArray" onmouseover="hints.show('catalogador.avanzado.categorias.form.modificar.general.param.datos.title')" onmouseout="hints.hide()"  styleId="modificarGeneralDatos"/>
                               </c:otherwise>
                           </c:choose>
                        </td>
                    </tr>

                    <tr>
                        <td>
                              <html:submit onmouseover="hints.show('modificarGeneral')" onmouseout="hints.hide()" styleId="form_submit">
                                  <bean:message key="catalogador.avanzado.categorias.form.modificar.general"/>
                              </html:submit>	
                        </td>
                    </tr>
                </tbody>
            </table>
        </html:form>
    </div>

</div>
 <br class="clr"/>
