            <xsd:complexType name="OdeVO">
                <xsd:annotation>     
                    <xsd:documentation> 
                        Objeto VO que representa un objeto Manifest de SCORM2004
                        simplificado. Para mapear objetos Manifest a OdeVO y viceversa
                        se emplea la librería Dozer. La configuración del mapeo se
                        define en un fichero de configuracion de Dozer.
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element name="identifier" nillable="false" type="xsd:string">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Identificador del Ode. Se corresponde con el atributo identifier
                                de la clase Manifest.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ruta" nillable="true" type="xsd:string">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Ruta relativa en la que se almacena el OdeVO. Toma valor cuando
                                el OdeVO es un Submanifiesto. Se corresponde con el atributo
                                base de la clase Manifest.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="codigoMEC" nillable="true" type="xsd:string">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Código MEC del Ode en edicion.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="organizacionPrincipal" nillable="false" type="xsd:string">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Identificador de la organización principal del OdeVO. Se
                                corresponde con el atributo default de la clase Organizations.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="catalogado" nillable="true" type="soapenc:boolean">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Variable booleana para marcar cuando un ODE ha sido catalogado.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="localizadorURL" nillable="true" type="xsd:string">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                URL de la ruta base del Ode.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="usuario" nillable="true" type="xsd:string">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Identificador del usuario propietario del ODE.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="conSecuencia" nillable="true" type="soapenc:boolean">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="submanifiestos" nillable="true" type="impl:ArrayOfOdeVO">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Array de submanifiestos (OdeVO) agregados al OdeVO. Se
                                corresponde con el atributo manifest de la clase Manifest.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>   
                    <xsd:element name="organizaciones" nillable="true" type="impl:ArrayOfOrganizacionVO">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Array de organizaciones contenidas en el OdeVO. Se corresponde
                                con el atributo organization de la clase Organizations.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>   
                    <xsd:element name="recursos" nillable="true" type="impl:ArrayOfRecursoVO">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Array de recursos del OdeVO. Se corresponde con el atributo
                                resource de la clase Resources.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>   
                    <xsd:element name="metadata" nillable="true" type="impl:MetadataVO">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Metadatos del Manifest. Se corresponde con el atributo metadata
                                de la clase Manifest.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>   
                    <xsd:element name="lom" nillable="false" type="impl:LomVO">
                        <xsd:annotation>     
                            <xsd:documentation> 
                                Lom asociado al OdeVO. Se corresponde con el objeto Lom
                                contenido en la clase Metadata del Manifest.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>   
                </xsd:sequence>
            </xsd:complexType>
